[run]
omit=
    .data/*
    .venv/*
    venv/*

    # Migrations are generated automatically and cannot be tested
    */migrations/*

    # Admin site is also autogenerated
    */admin.py

    # Don't need to test it too
    */apps.py

    # Schemas are used only for openapi docs generation
    */schema.py

    # Do not need to test manage.py
    manage.py

    */asgi.py
    */wsgi.py
branch = True

[report]
show_missing = True
skip_empty = True
